                controls: ['calendar'],
                select:'range',
                touchUi: true
            });

            function getVal() {
                const val = document.querySelector('#mobi-picker').value;
                if(val){
                    const rango_fechas = val.split("-")
                    console.log(rango_fechas[0].trim());
                    console.log(rango_fechas[1].trim());
                }
            }

            let ph = []
            let fechas = []
            let ce = []

            let temperatura = []
            let humedad = []
            let presion = []
            let luminosidad = []
            let fechas_red = []

            const filtro_datos_baston = '<%= filtro_datos_baston %>';
            const ph_values = '<%= ph %>';
            const fecha_values = '<%= fechas %>';
            const ce_values = '<%= ce %>';

            const temperatura_values = '<%= temperatura %>';
            const humedad_values = '<%= humedad %>';
            const presion_values = '<%= presion %>';
            const luminosidad_values = '<%= luminosidad%>';
            const fechas_red_values = '<%= fechas_red%>';

            ph = ph_values.split(','); 
            fechas = fecha_values.split(',');
            ce = ce_values.split(',');
            temperatura = temperatura_values.split(',');
            humedad = humedad_values.split(',');
            presion = presion_values.split(',');
            luminosidad = luminosidad_values.split(',');
            fechas_red = fechas_red_values.split(',');

            chartIt();

            getVal();

            async function chartIt(){

                await '<%= filtro_datos_baston %>';

                var ctx1 = document.getElementById("chart-pH").getContext("2d");
                var gradientStroke1 = ctx1.createLinearGradient(0, 230, 0, 50);

                gradientStroke1.addColorStop(1, 'rgba(203,12,159,0.2)');
                gradientStroke1.addColorStop(0.2, 'rgba(72,72,176,0.0)');
                gradientStroke1.addColorStop(0, 'rgba(203,12,159,0)'); //purple colors

                var gradientStroke2 = ctx1.createLinearGradient(0, 230, 0, 50);

