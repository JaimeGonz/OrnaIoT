<div>

    <% if (filtro_datos_baston) { %>
        
        <script src="/assets/js/plugins/chartjs.min.js"></script>
        <script src="../assets/js/mobiscroll.javascript.min.js"></script>

        <script src="https://cdnjs.cloudflare.com/ajax/libs/hammer.js/2.0.8/hammer.min.js" 
        integrity="sha512-UXumZrZNiOwnTcZSHLOfcTs0aos2MzBWHXOHOuB0J/R44QB0dwY5JgfbvljXcklVf65Gc4El6RjZ+lnwd2az2g==" 
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>

            <!-- Zoom -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/chartjs-plugin-zoom/1.2.1/chartjs-plugin-zoom.min.js" 
        integrity="sha512-klQv6lz2YR+MecyFYMFRuU2eAl8IPRo6zHnsc9n142TJuJHS8CG0ix4Oq9na9ceeg1u5EkBfZsFcV3U7J51iew==" 
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>

        <script src="https://momentjs.com/downloads/moment.min.js"></script>

        <script>

            mobiscroll.setOptions({
                locale: mobiscroll.localeEs,
                themeVariant: 'light'
            });

            mobiscroll.datepicker('#mobi-picker_ph', {
                controls: ['calendar'],
                select:'range',
                touchUi: true
            });

            mobiscroll.datepicker('#mobi-picker_ce', {
                controls: ['calendar'],
                select:'range',
                touchUi: true
            });

            mobiscroll.datepicker('#mobi-picker_temperatura', {
                controls: ['calendar'],
                select:'range',
                touchUi: true
            });

            mobiscroll.datepicker('#mobi-picker_humedad', {
                controls: ['calendar'],
                select:'range',
                touchUi: true
            });

            mobiscroll.datepicker('#mobi-picker_luminosidad', {
                controls: ['calendar'],
                select:'range',
                touchUi: true
            });

            mobiscroll.datepicker('#mobi-picker_presion', {
                controls: ['calendar'],
                select:'range',
                touchUi: true
            });

            moment.locale('es');

            let myChart;
            let myChart2;
            let myChart3;
            let myChart4;
            let myChart5;
            let myChart6;

            let ph = []
            let fechas = []
            let ce = []

            let temperatura = []
            let humedad = []
            let presion = []
            let luminosidad = []
            let fechas_red = []

            const filtro_datos_baston = '<%= filtro_datos_baston %>';

            const ph_values = '<%= ph %>';
            const fecha_values = '<%= fechas %>';
            const ce_values = '<%= ce %>';

            const temperatura_values = '<%= temperatura %>';
            const humedad_values = '<%= humedad %>';
            const presion_values = '<%= presion %>';
            const luminosidad_values = '<%= luminosidad%>';
            const fechas_red_values = '<%= fechas_red%>';

            ph = ph_values.split(','); 
            fechas = fecha_values.split(',');
            ce = ce_values.split(',');
            temperatura = temperatura_values.split(',');
            humedad = humedad_values.split(',');
            presion = presion_values.split(',');
            luminosidad = luminosidad_values.split(',');
            fechas_red = fechas_red_values.split(',');

            // DATOS RED DE SENSORES
            const datos_red = '<%- JSON.stringify(datos_red) %>';
            const json_parse_red = JSON.parse(datos_red);

            // DATO BASTON PORTABLE
            const datos_baston= '<%- JSON.stringify(indicadores_baston) %>';
            const json_parse_baston = JSON.parse(datos_baston);

            chartIt();

            function filtro_ph() {
                const val = document.querySelector('#mobi-picker_ph').value;
                let data_ph = []
                let fechas_baston_ph = []
                
                if(val){

                    const rango_fechas = val.split("-")
                    const inicio = moment(rango_fechas[0].trim(), "DD/MM/YYYY");
                    const fin = moment(rango_fechas[1].trim(), "DD/MM/YYYY");

                    json_parse_baston.forEach( (element) => {

                        const time = moment(moment(element.dataTime).format('DD/MM/YYYY'), "DD/MM/YYYY");

                        if ( (time >= inicio) && (time <= fin) ) {
                            data_ph.push(element.ph);
                            fechas_baston_ph.push(moment(element.dataTime).format('L'));
                        }

                    });

                    var ctx1 = document.getElementById("chart-pH").getContext("2d");

                    if (myChart) {
                        myChart.destroy();
                    }

                    var gradientStroke1 = ctx1.createLinearGradient(0, 230, 0, 50);

                    myChart = new Chart(ctx1, {
                        type: "line",
                        data: {
                            labels: fechas_baston_ph,
                            datasets: [{
                                label: 'pH',
                                tension: 0.4,
                                pointRadius: 0,
                                borderColor: "#3A416F",
                                borderWidth: 3,
                                data: data_ph,
                                maxBarThickness: 6
                            }],
                        },
                        options: {
                            responsive: true,
                            maintainAspectRatio: false,
                            plugins: {
                                legend: {
                                    display: false,
                                },
                                zoom: {
                                    zoom: {
                                        wheel: {
                                            enabled: true,
                                        }
                                    }
                                }
                            },
                            interaction: {
                                intersect: false,
                                mode: 'index',
                            },
                            scales: {
                                y: {
                                    grid: {
                                        drawBorder: false,
                                        display: true,
                                        drawOnChartArea: true,
                                        drawTicks: false,
                                        borderDash: [5, 5]
                                    },
                                    ticks: {
                                        display: true,
                                        padding: 10,
                                        color: '#b2b9bf',
                                        font: {
                                            size: 11,
                                            family: "Open Sans",
                                            style: 'normal',
                                            lineHeight: 2
                                        },
                                    }
                                },
                                x: {
                                    grid: {
                                        drawBorder: false,
                                        display: false,
                                        drawOnChartArea: false,
                                        drawTicks: false,
                                        borderDash: [5, 5]
                                    },
                                    ticks: {
                                        display: true,
                                        color: '#b2b9bf',
                                        padding: 20,
                                        font: {
                                            size: 11,
                                            family: "Open Sans",
                                            style: 'normal',
                                            lineHeight: 2
                                        },
                                    }
                                },
                            },
                        },
                    });
                }
                
            }

            function filtro_ce() {
                const val = document.querySelector('#mobi-picker_ce').value;
                let data_ce = []
                let fechas_baston_ce = []
                
                if ( val ) {

                    const rango_fechas = val.split("-")
                    const inicio = moment(rango_fechas[0].trim(), "DD/MM/YYYY");
                    const fin = moment(rango_fechas[1].trim(), "DD/MM/YYYY");

                    json_parse_baston.forEach( (element) => {

                        const time = moment(moment(element.dataTime).format('DD/MM/YYYY'), "DD/MM/YYYY");

                        if ( (time >= inicio) && (time <= fin) ) {
                            data_ce.push(element.ce);
                            fechas_baston_ce.push(moment(element.dataTime).format('L'));
                        }

                    });

                    var ctx2 = document.getElementById("chart-ce").getContext("2d");

                    if (myChart2) {
